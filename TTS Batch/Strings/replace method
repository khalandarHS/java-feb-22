package com.company;

class Main {
    public static void main(String[] args) {
        StringBuilder sb = new StringBuilder("hello");
        sb.append("world");

        System.out.println(sb);
        
        sb.insert(5, " ;ok[]");
        System.out.println(sb);

        sb.replace(6, 8, "");
        System.out.println(sb);

    }
}